//Program 1


//             0      1     2      3       4
let names = ["ram","sam","komal","mohan","hari"]
console.log (names[0])
console.log (names[1])


//Program 2

for (let i = 0; i < names.length; i++){
    console.log(i)
    console.log(names[i])
}


//Program 3

for (let i = 4; i >= 0; i--){
    console.log(i)
    console.log(names[i])
}


//Program 4

let cities = ["pune","bangalore","kolkata"]
console.log (cities[0])

//while loop
let j = 0
while (j <= 2){
    console.log (j)
    console.log (cities[j])
    j++
}


//Program 5

let j1 = 2
while (j1 >= 0){
    //console.log(j)
    console.log (cities[j1])
    j1--
}



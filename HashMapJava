//Javascript object-----------property value and key value

let names = ["ram","joshi",30,40]
let info = {
    firstName : "ram",
    lastName : "joshi",
    age : 30,
    rollNo : 40
}

//retrieve (dot notation and bracket notation)

console.log (info.firstName)
console.log (info['firstName'])

//add (dot notation and bracket notation)

info.city ="kathmandu"
info['language']= "nepali"
console.log (info)

//update (dot notation and bracket notation)

info.city = "pokhara"
info["language"] = "newari"
console.log (info)

//delete (dot notation and bracket notation)

delete info.city
delete info['language']
console.log (info)

//In json(javascript object notation),wrap the property in double quote)

/*JSON: JSON follows a specific notation that uses curly braces {} to represent objects, square brackets [] for arrays, and uses a 
        colon : to separate keys and values in objects.*/

